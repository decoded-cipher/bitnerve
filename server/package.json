    "postgres": "^3.4.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "bun-types": "^1.3.1",
    "drizzle-kit": "^0.31.6",
    "typescript": "^5.9.3"
  }
